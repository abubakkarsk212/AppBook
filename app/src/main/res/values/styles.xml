<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#00bcd4</item>
        <item name="colorPrimaryDark">#0277bd</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="noactionbar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">#40c4ff</item>
    </style>

    <style name="titleHeading">
        <item name="android:textSize">22sp</item>

        <item name="android:textColor">#000000</item>
        <item name="android:background">#FFFFFF</item>
    </style>

    <style name="text">
        <item name="android:textSize" >18sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:height">80dp</item>
        <item name="android:background">@drawable/text_selector</item>
    </style>
    <style name="headingContact">

        <item name="android:textColor">#3f51b5</item>
        <item name="android:background">#e0e0e0</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="teztContact">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#64dd17</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="headingWriting">
        <item name="android:textColor">#f50057</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>
    <style name="textWritting">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">18sp</item>
    </style>


</resources>
