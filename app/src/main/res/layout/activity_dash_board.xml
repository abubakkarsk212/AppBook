<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="16dp"
    android:paddingTop="16dp"
    tools:context="com.popo.as.appbook.DashBoard"
    android:background="#f5f5f5">
    <TextView
        android:paddingLeft="16dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="Explore Apps"
        style="@style/titleHeading"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:layout_marginBottom="16dp"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:orientation="horizontal">


        <TextView
            android:text="Banking"
            style="@style/text"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:id="@+id/banking"
            android:layout_height="80dp"
            android:fontFamily="sans-serif-condensed"
            android:gravity="center"
            />

        <View
            android:layout_width="16dp"
            android:layout_height="match_parent"
            android:background="#ffffff"
            >
        </View>

        <TextView
            android:id="@+id/wallet"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Wallets"
            android:gravity="center"
            style="@style/text"
            android:fontFamily="sans-serif-condensed"
            android:layout_height="80dp" />
    </LinearLayout>


    <TextView
        android:layout_marginTop="16dp"
        android:paddingLeft="16dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="App you must try"
        style="@style/titleHeading"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:layout_marginBottom="16dp"
        android:id="@+id/textView" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <View
        android:layout_marginLeft="30dp"
        android:layout_width="120dp"
        android:layout_height="6dp"
        android:background="#ffffff"
        >
    </View>
        <View
            android:layout_marginRight="30dp"
            android:layout_width="120dp"
            android:layout_height="6dp"
            android:background="#ffffff"
            android:layout_alignParentRight="true"
            >
    </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="120dp"
            android:layout_height="165dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="16dp"
            android:layout_alignParentRight="true"
            android:background="#f5f5f5">
            <com.appnext.nativeads.NativeAdView
                android:id="@+id/iconad_view1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/white"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iconad_icon1"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginTop="20dp"
                        android:layout_gravity="center_horizontal"
                        tools:src="@drawable/ic_launcher_foreground" />

                    <TextView
                        android:id="@+id/iconad_title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="15dp"
                        android:textSize="9sp" />

                    <Button
                        android:id="@+id/iconad_install1"
                        android:layout_width="75dp"
                        android:layout_height="30dp"
                        android:background="@drawable/rounded_button"
                        android:text="install"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal"
                        android:textSize="9sp"/>

                </LinearLayout>
            </com.appnext.nativeads.NativeAdView>
        </RelativeLayout>

    <RelativeLayout
        android:layout_width="120dp"
        android:layout_height="165dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        android:background="#f5f5f5">
        <com.appnext.nativeads.NativeAdView
            android:id="@+id/iconad_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/white"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/iconad_icon"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center_horizontal"
                    tools:src="@drawable/ic_launcher_foreground" />

                <TextView
                    android:id="@+id/iconad_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                   android:layout_gravity="center_horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="15dp"
                    android:textSize="9sp" />

                <Button
                    android:id="@+id/iconad_install"
                    android:layout_width="75dp"
                    android:layout_height="30dp"
                    android:background="@drawable/rounded_button"
                    android:text="install"
                    android:textColor="#ffffff"
                    android:layout_gravity="center_horizontal"
                    android:textSize="9sp"/>

            </LinearLayout>
        </com.appnext.nativeads.NativeAdView>
    </RelativeLayout>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#ffffff"
        >
    </View>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f5f5f5">

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            />

        <com.appnext.nativeads.NativeAdView
            android:id="@+id/na_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="20dp"
            android:background="@android:color/white"
            android:padding="10dp">


            <RelativeLayout
                android:id="@+id/na_ad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <LinearLayout
                    android:id="@+id/icoon"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <ImageView
                    android:id="@+id/na_icon"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="10dp"
                    tools:src="@drawable/ic_launcher_foreground" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp">

                <TextView
                    android:id="@+id/na_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="15dp"
                    android:gravity="left"
                    android:textSize="16sp"
                    android:textColor="#000000"/>

                <TextView
                    android:id="@+id/rating"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:textSize="16sp" />

                    <TextView
                        android:id="@+id/description"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"
                        tools:text="TextView" />

                </LinearLayout>
                </LinearLayout>
                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="4dp"
                    android:layout_below="@id/icoon"
                    />
                <com.appnext.nativeads.MediaView
                    android:id="@+id/na_media"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:layout_marginTop="4dp"
                    android:layout_below="@id/icoon"
                    />
                <Button
                    android:id="@+id/install"
                    android:layout_width="104dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/rounded_button"
                    android:layout_alignParentRight="true"
                    android:text="install"
                    android:textColor="#ffffff"
                    android:layout_toEndOf="@id/imageView"
                    android:layout_below="@id/na_media"/>




            </RelativeLayout>
        </com.appnext.nativeads.NativeAdView>
    </RelativeLayout>
    <View
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="6dp"
        android:background="#ffffff"
        >
    </View>

    <TextView
        android:layout_marginTop="16dp"
        android:paddingLeft="16dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="Suggested for you"
        style="@style/titleHeading"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:layout_marginBottom="16dp"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <View
            android:layout_marginLeft="30dp"
            android:layout_width="120dp"
            android:layout_height="6dp"
            android:background="#ffffff"
            >
        </View>
        <View
            android:layout_marginRight="30dp"
            android:layout_width="120dp"
            android:layout_height="6dp"
            android:background="#ffffff"
            android:layout_alignParentRight="true"
            >
        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="120dp"
            android:layout_height="165dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="16dp"
            android:layout_alignParentRight="true"
            android:background="#f5f5f5">
            <com.appnext.nativeads.NativeAdView
                android:id="@+id/iconad_view3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/white"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iconad_icon3"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginTop="20dp"
                        android:layout_gravity="center_horizontal"
                        tools:src="@drawable/ic_launcher_foreground" />

                    <TextView
                        android:id="@+id/iconad_title3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="15dp"
                        android:textSize="9sp" />

                    <Button
                        android:id="@+id/iconad_install3"
                        android:layout_width="75dp"
                        android:layout_height="30dp"
                        android:background="@drawable/rounded_button"
                        android:text="install"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal"
                        android:textSize="9sp"/>

                </LinearLayout>
            </com.appnext.nativeads.NativeAdView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="120dp"
            android:layout_height="165dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:background="#f5f5f5">
            <com.appnext.nativeads.NativeAdView
                android:id="@+id/iconad_view2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/white"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iconad_icon2"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginTop="20dp"
                        android:layout_gravity="center_horizontal"
                        tools:src="@drawable/ic_launcher_foreground" />

                    <TextView
                        android:id="@+id/iconad_title2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="15dp"
                        android:textSize="9sp" />

                    <Button
                        android:id="@+id/iconad_install2"
                        android:layout_width="75dp"
                        android:layout_height="30dp"
                        android:background="@drawable/rounded_button"
                        android:text="install"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal"
                        android:textSize="9sp"/>

                </LinearLayout>
            </com.appnext.nativeads.NativeAdView>
        </RelativeLayout>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#ffffff"
        >
    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">
        <ProgressBar
            android:id="@+id/progressBar1"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            />
    <com.appnext.nativeads.NativeAdView
        android:id="@+id/startapp_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8dp">

        <ImageView
            android:id="@+id/startappicon"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="4dp"
            android:layout_marginBottom="4dp"
            android:layout_marginTop="4dp"
            tools:src="@drawable/ic_launcher_foreground"
            />
        <TextView
            android:id="@+id/startapp_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/startappicon"
            android:gravity="left"
            android:textSize="12sp" />
        <TextView
            android:id="@+id/startapp_description"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/startapp_title"
            android:layout_toRightOf="@id/startappicon"
            android:textSize="12sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="112dp"
            android:layout_marginTop="4dp"
            tools:text="TextView" />

        <Button
            android:id="@+id/startapp_install"
            android:layout_width="75dp"
            android:layout_height="35dp"
            android:layout_alignParentEnd="true"
            android:background="@drawable/rounded_button"
            android:textSize="12sp"
            android:layout_marginRight="6dp"
            android:text="install"
            android:layout_centerVertical="true"
            android:textColor="#ffffff" />
        </RelativeLayout>
    </com.appnext.nativeads.NativeAdView>
    </RelativeLayout>

    <TextView
        android:layout_marginTop="16dp"
        android:paddingLeft="16dp"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="Feel free to ask!"
        style="@style/titleHeading"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:layout_marginBottom="16dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="16dp"
        android:paddingRight="16dp">

        <TextView
           android:onClick="feedBack"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Feedback"
            style="@style/text"
            android:fontFamily="sans-serif-condensed"
            android:gravity="center"
            android:layout_height="80dp" />

        <View
            android:layout_width="16dp"
            android:layout_height="match_parent"
            android:background="#ffffff"
            >
        </View>
        <TextView
            android:id="@+id/contactus"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:text="Contact us"
            android:gravity="center"
            style="@style/text"
            android:fontFamily="sans-serif-condensed"
            android:layout_height="80dp" />

    </LinearLayout>
</LinearLayout>
</ScrollView>